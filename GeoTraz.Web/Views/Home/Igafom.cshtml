<div>
    <div class="card radius-1 pt-0">
        <div class="card-header">
            <h5 class="card-title text-110 text-dark-tp2">
                @*@ViewBag.Message*@
                IGAFOM
            </h5>


        </div><!-- /.card-header -->



    </div>
</div>
<div class="cards-container ">
    <div class="card  ">
        <div class="card-header p-0 pl-2 pt-1">
            <h5 class="card-title text-black-50">
                <i class="fa fa-filter mr-2px "></i>  Filtros de Búsqueda
            </h5>
        </div>
        <div class="card-body p-1">
            <div class="p-2">
                <div class="form-row">
                    <div class="form-group col-md-2">
                        <label>RUC</label>
                        <input type="text" class="form-control input-number" id="txtRucFiltrar" placeholder="RUC">
                    </div>

                    <div class="form-group col-md-4">
                        <label for="inputCity">Proveedor</label>
                        <input type="text" class="form-control" placeholder="Proveedor" id="txtRazonSocialFiltrar">
                    </div>
                    <div class="form-group col-md-3">
                        <label for="inputZip">Código Concesión</label>
                        <input type="text" class="form-control" id="txtCodConcesionFiltrar" placeholder="Código Concesión">
                    </div>

                    <div class="form-group col-md-1">
                        <label>Buscar</label><br />
                        <button class="btn btn-success text-white" id="btnSearch"><i class="fas fa-search text-110 align-text-bottom mr-1"></i> Buscar</button>
                    </div>
                    <div class="form-group col-md-1">
                        <label>Acción</label><br />
                        <button class="btn btn-warning text-white" id="btnReset"><i class="fas fa-sync-alt text-110 align-text-bottom mr-1"></i> Limpiar</button>
                    </div>
                    <div class="form-group col-md-1">
                        <label for="">Exportar</label><br />
                        <button class="btn btn-secondary text-white" id="btnExportar"><i class="fa fa-download text-110 align-text-bottom mr-1"></i> Exportar</button>
                    </div>


                </div>
                <div class="form-row">
                    <div class="form-group col-md-3">
                        <label for="inputZip">Nombre Concesión</label>
                        <input type="text" class="form-control" id="txNombreConceFiltrar" placeholder="Nombre Concesión">
                    </div>
                    <div class="form-group col-md-2">
                        <label for="inputZip">Sede</label>
                        <select id="cmbSedeFiltrar" class="form-control">
                        </select>
                    </div>
                    <div class="form-group col-md-4">
                    </div>
  


                </div>
                    <div class="form-row" style="display:none">

                        <div class="form-group col-md-3">
                            <label for="inputZip">Nombre del Derecho Minero</label>
                            <input type="text" class="form-control" id="txtNomDerechoMineroFiltrar" placeholder="Nombre del Derecho Minero">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Trazabilidad</label>
                            <select id="cmbResultadoFiltrar" class="form-control">
                                <option selected value="0">TODOS...</option>
                                <option>TRAZABLE</option>
                                <option>PENDIENTE</option>
                                <option>NO TRAZABLE</option>
                            </select>
                        </div>
                        <div class="form-group col-md-2">
                            <label for="form-field-mask-1">
                                Fecha de Reinfo
                            </label>
                            <div class="input-group">
                                <input type="date" class="form-control form-control" id="txtFechaReinfoFiltrar" />
                            </div>
                        </div>


                    </div>

                </div>
        </div>
    </div>
</div>



@{
    ViewData["Title"] = "Home Page";
}

<div>
    <div class="card radius-1 pt-0">
        <div class="card-header">
            <h5 class="card-title text-110 text-dark-tp2">
                @*@ViewBag.Message*@
                CATÁLOGO DE DOCUMENTOS
            </h5>


        </div><!-- /.card-header -->



    </div>
</div>
<div class="cards-container ">
    <div class="card  ">
        <div class="card-header p-0 pl-2 pt-1">
            <h5 class="card-title text-black-50">
                <i class="fa fa-filter mr-2px "></i>  Filtros de Búsqueda
            </h5>
        </div>
        <div class="card-body p-1">
            <div class="p-2">
                <div class="form-row">
                    <div class="form-group col-md-2">
                        <label>RUC</label>
                        <input type="text" class="form-control input-number" id="txtRucFiltrar" placeholder="RUC">
                    </div>
                    <div class="form-group col-md-3">
                        <label for="inputCity">Razón Social</label>
                        <input type="text" class="form-control" placeholder="Razón Social" id="txtRazonSocialFiltrar">
                    </div>
                    <div class="form-group col-md-2">
                        <label for="inputZip">Código Concesión</label>
                        <input type="text" class="form-control" id="txtCodConcesionFiltrar" placeholder="Código Concesión">
                    </div>
                    <div class="form-group col-md-2">
                        <label for="inputZip">Nombre Concesión</label>
                        <input type="text" class="form-control" id="txNombreConceFiltrar" placeholder="Nombre Concesión">
                    </div>
                    <div class="form-group col-md-1">
                        <label>Buscar</label><br />
                        <button class="btn btn-success text-white" id="btnSearch"><i class="fas fa-search text-110 align-text-bottom mr-1"></i> Buscar</button>
                    </div>
                    <div class="form-group col-md-1">
                        <label for="inputCity">Acción</label><br />
                        <button class="btn btn-warning text-white" id="btnReset"><i class="fas fa-sync-alt text-110 align-text-bottom mr-1"></i> Limpiar</button>
                    </div>
                    <div class="form-group col-md-1" style="display:none">
                        <label for="">Exportar</label><br />
                        <button class="btn btn-secondary text-white" id="btnExportar"><i class="fa fa-download text-110 align-text-bottom mr-1"></i> Exportar</button>
                    </div>
                </div>
                <div class="form-row" style="display:none">
                    <div class="form-group col-md-3">
                        <label for="inputZip">Nombre del Derecho Minero</label>
                        <input type="text" class="form-control" id="txtNomDerechoMineroFiltrar" placeholder="Nombre del Derecho Minero">
                    </div>
                    <div class="form-group col-md-2">
                        <label>Trazabilidad</label>
                        <select id="cmbResultadoFiltrar" class="form-control">
                            <option selected value="0">TODOS...</option>
                            <option>TRAZABLE</option>
                            <option>PENDIENTE</option>
                            <option>NO TRAZABLE</option>
                        </select>
                    </div>
                    <div class="form-group col-md-2">
                        <label for="form-field-mask-1">
                            Fecha de Reinfo
                        </label>
                        <div class="input-group">
                            <input type="date" class="form-control form-control" id="txtFechaReinfoFiltrar" />
                        </div>
                    </div>
                    <div class="form-group col-md-2">
                        <label for="form-field-mask-1">
                            Sede
                        </label>
                        <div class="input-group">
                            <select id="cmbSedeFiltrar" class="form-control">
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<link rel="stylesheet" type="text/css" href="./node_modules/bootstrap-table/dist/bootstrap-table.css">
<div>
    <div class="card radius-1 pt-2 pl-3 pb-0">
        <div class="">
            <table class="table text-dark-m2 text-95" id="table">
            </table>
        </div><!-- /.card-body -->
    </div>
</div>
@*<embed src="~/firmas/SISTEMA DE INSPECCION DE LABORES MINERAS - TRAZABILIDAD.pdf" width="600" height="675"
       type="application/pdf">*@
<div class="modal fade" id="myModalDeclaracionMinera" role="dialog">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Catálogo de Documentos</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="cards-container p-0">
                <div class="card  p-0">
                    <div class="card-body p-2">
                        <div class="p-0">
                            <input type="text" id="TxtIndicador" style="display:none" />
                            <input type="text" id="txtId_IdReinfo" style="display:none" />
                            <table class="table" border="1">
                                <tr>
                                    <td colspan="1" style="font-size:13px"><label><strong>RAZÓN SOCIAL:</strong></label></td>
                                    <td colspan="3"><label id="txtRazonSocial"></label></td>
                                    <td><label style="font-size:13px"><strong>RUC:</strong></label></td>
                                    <td><label id="txtRucProveedor"></label></td>
                                </tr>
                                <tr>
                                    <td><label style="font-size:13px"><strong>DERECHO MINERO:</strong></label></td>
                                    <td> <label id="txtNomDerechoMinero"></label></td>
                                    <td><label style="font-size:13px"><strong>CÓDIGO CONCESIÓN:</strong></label></td>
                                    <td> <label id="txtCodConcesion"></label></td>
                                    <td><label style="font-size:13px"><strong>NOMBRE CONCESIÓN:</strong></label></td>
                                    <td> <label id="txtNomConcesion"></label></td>
                                </tr>
                            </table>
                            <div class="col-12 col-md-12 p-0">
                                <div class="card border-0">
                                    <div class="card-header bgc-info p-0">
                                        <h3 class="card-title text-130 text-white pt-1 pl-2">Archivos asociados</h3>
                                    </div>
                                    <div class="card-body bgc-white border-1 border-t-0 brc-info">
                                        <div class="jqtree tree-dotted" id="id-jqtree-files"></div>
                                    </div>
                                </div>
                            </div>
                        </div><!-- /.card-body -->
                    </div>
                </div>
            </div>
            @*<div>
                    <figure class="highcharts-figure">
                        <div id="container"></div>
                    </figure>
                </div>*@
            <div class="col-md-12 p-2">
                <div>
                    <center>
                        @*<button type="button" class="btn btn-success" id="btnAgregarEditarDeclaracion" onclick="AgregarOeditarDeclaracion()">Guardar</button>*@
                        <button type="button" class="btn btn-danger" data-dismiss="modal" id="btnCancelar"><i class="fa fa-times-circle text-110 align-text-bottom mr-1"></i> Cerrar</button><br /><br />
                        @*<button type="button" class="btn btn-success" data-dismiss="modal">Guardar</button>*@
                    </center>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal" id="myModalLoading" tabindex="-1" role="dialog" style="display: block;" hidden>
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-body">
                <div class=" d-flex flex-column justify-content-between align-items-center">
                    <p class="pt-2" style="color:darkblue">Por favor espere.....</p>
                    <div class="fas fa-spinner fa-spin text-purple-m1 fa-2x mb-3"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal" id="myModalLoading2" tabindex="-1" role="dialog" style="display: block;" hidden>
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-body">
                <div class=" d-flex flex-column justify-content-between align-items-center">
                    <p class="pt-2 text-110" style="color:indigo">CARGANDO REPORTE.....</p>
                    <i class="fas fa-sync fa-spin text-purple-m1 text-170 mb-3"></i>
                </div>
            </div>
        </div>
    </div>
</div>
